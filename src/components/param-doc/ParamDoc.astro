---
interface Props {
  name: string;
}

const { name } = Astro.props;
---

<Fragment>
  <div class="param-doc-cell"><code>{name}</code></div>
  <div class="param-doc-cell">-</div>
  <div class="param-doc-cell">
    <slot />
  </div>
</Fragment>

<style>
  .param-doc-cell {
    margin-top: 0;
  }
</style>
